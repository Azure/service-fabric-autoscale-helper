<?xml version="1.0"?>
<!-- To create a nuget package
     nuget pack AutoscaleManager.nuspec -basepath pkg\Release -Properties RuntimeIdentifier=linux-x64 -->
<package>
  <metadata minClientVersion="3.3.0">
    <id>AutoscaleManager-$RuntimeIdentifier$</id>
    <authors>raunakpandya, vipulm-msft, prashantbhutani90</authors>
    <version>1.0.0</version>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This package contains the AutoscaleManager Application (Config files and binaries).</description>
  <contentFiles>
    <files include="**/*.*" buildAction="None" copyToOutput="true" />
  </contentFiles>
  </metadata>
  <files>
    <file src="**.*" target="contentFiles\any\any" />
  </files>
</package>